html,
body {
  height: 100%;
}
body {
  background-image: url("../images/LEDbg.jpg");
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin: 0;
  font-family: Helvetica, sans-serif;
  overflow: hidden;
  font-family: "ºÚÌå";
}
a {
  color: #ffffff;
}
#info {
  position: absolute;
  width: 100%;
  color: #ffffff;
  padding: 5px;
  font-family: Monospace;
  font-size: 13px;
  font-weight: bold;
  text-align: center;
  z-index: 1;
}
#menu {
  position: absolute;
  bottom: 20px;
  width: 100%;
  text-align: center;
}
.element {
  width: 120px;
  height: 160px;
  box-shadow: 0px 0px 12px rgba(0, 255, 255, 0.5);
  border: 1px solid rgba(127, 255, 255, 0.25);
  text-align: center;
  cursor: default;
  border: 2px solid #A47B45;
}
.element:hover {
  box-shadow: 0px 0px 12px rgba(0, 255, 255, 0.75);
  border: 1px solid rgba(127, 255, 255, 0.75);
}
.element .number {
  position: absolute;
  top: 0px;
  right: 0px;
  width: 100%;
  height: 60%;
  font-size: 12px;
  color: rgba(127, 255, 255, 0.75);
}
.element .number img {
  display: block;
  width: 100%;
  height: 100%;
}
.element .symbol {
  position: absolute;
  top: 40px;
  left: 0px;
  right: 0px;
  font-size: 60px;
  font-weight: bold;
  color: #2E465F;
}
.element img {
  display: block;
  width: 100%;
}
.element .details {
  position: absolute;
  width: 100%;
  text-align: center;
  bottom: 5px;
  left: 0px;
  right: 0px;
  font-size: 22px;
  color: #000000;
  font-weight: bold;
  z-index: 100;
  font-family: "ºÚÌå";
}
button {
  color: rgba(127, 255, 255, 0.75);
  background: transparent;
  outline: 1px solid rgba(127, 255, 255, 0.75);
  border: 0px;
  padding: 5px 10px;
  cursor: pointer;
}
button:hover {
  background-color: rgba(0, 255, 255, 0.5);
}
button:active {
  color: #000000;
  background-color: rgba(0, 255, 255, 0.75);
}
#canvas {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 900;
}
